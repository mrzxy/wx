@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}



.myiconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}




.icon-sousuo:before { content: "\e620"; }

.icon-fenlei:before { content: "\e621"; }


.icon-meiyuan:before { content: "\e605"; }

.icon-fenxiang:before { content: "\e62c"; font-size: 25px; }

.icon-qian:before { content: "\e604"; }


.hr {
    height: 1rpx;
    width: 100%;
    border-top: 1px solid #F5F5F5;
}

.text-input, .textarea{
  background-color: #F5F5F5
}

